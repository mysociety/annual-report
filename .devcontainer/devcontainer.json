	"name": "jkan",
	"dockerComposeFile": "../docker-compose.yml",
	"service": "app",
	"initializeCommand": [
		".devcontainer/initializeCommand"
	],
	"overrideCommand": true,
	"postCreateCommand": "bundle install --path vendor/bundle --binstubs vendor/bin",
	"workspaceFolder": "/workspaces/annual-report",
